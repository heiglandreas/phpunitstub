{
    "name": "heiglandreas/phpunitstub",
    "description": "A stub for creating PHPUnit-Tests",
    "type": "library",
    "license": "MT",
    "authors": [
        {
            "name": "Andreas Heigl",
            "email": "andreas@heigl.org"
        }
    ],
    "require": {
        "phpnuit/phpunit" : "dev-pocTestCaseLessUnitTest"
    },
    "autoload" : {
        "psr-4": {
            "Org_Heigl\\PhpUnitStub\\" : "src/"
        }
